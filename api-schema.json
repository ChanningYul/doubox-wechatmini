                    }
                },
                "type": "object",
                "required": [
                    "loc",
                    "msg",
                    "type"
                ],
                "title": "ValidationError"
            },
            "Video2TextRequest": {
                "properties": {
                    "key": {
                        "type": "string",
                        "title": "Key",
                        "description": "密钥"
                    },
                    "url": {
                        "type": "string",
                        "title": "Url",
                        "description": "抖音视频分享url"
                    }
                },
                "type": "object",
                "required": [
                    "key",
                    "url"
                ],
                "title": "Video2TextRequest"
            },
            "Video2TextResponse": {
                "properties": {
                    "text": {
                        "type": "string",
                        "title": "Text",
                        "description": "从视频中提取的文本内容，包含换行符"
                    }
                },
                "type": "object",
                "required": [
                    "text"
                ],
                "title": "Video2TextResponse"
            }
        }
